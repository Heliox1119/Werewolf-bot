{
  "name": "werewolf-bot",
  "version": "3.1.0",
  "description": "Discord Werewolf game bot with voice control and ambient audio",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "health": "node scripts/health-check.js",
    "clear-commands": "node scripts/clear_commands.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "backup": "node -e \"const B=require('./database/backup');const D=require('./database/db');const db=new D();B.initialize(db);B.getInstance().performBackup().then(f=>console.log('Backup:',f))\""
  },
  "keywords": [
    "discord",
    "bot",
    "werewolf",
    "game",
    "voice"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@discordjs/voice": "^0.19.0",
    "@snazzah/davey": "^0.1.9",
    "better-sqlite3": "^11.0.0",
    "chalk": "^4.1.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.4",
    "ejs": "^4.0.1",
    "express": "^5.2.1",
    "express-session": "^1.19.0",
    "ffmpeg-static": "^5.3.0",
    "helmet": "^8.1.0",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "socket.io": "^4.8.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0"
  }
}
